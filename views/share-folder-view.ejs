<div class="library-layout">
    <% if (locals.folderInfo) { %>
    <div class="">
      <% if(folderInfo.has_parent_folder && folderInfo.parentFolderId){ %>
      <a
        href="/share/view?id=<%= folderInfo.req_id %>&folderid=<%= folderInfo.parentFolderId %>"
        class="lib-back lib-icon"
      >
        <svg
          xmlns="http://www.w3.org/2000/svg"
          width="30"
          height="30"
          fill="currentColor"
          class="bi bi-arrow-left-circle"
          viewBox="0 0 16 16"
